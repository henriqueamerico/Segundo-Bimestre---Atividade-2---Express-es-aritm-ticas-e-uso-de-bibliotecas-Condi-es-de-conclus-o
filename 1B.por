programa {
  funcao inicio() {
    inteiro horas, dias, anos, decada, min_decada
    
    escreva("Irei te falar quantos minutos tem em uma decada! \n")
    horas = 60
    dias = 24
    anos = 365
    decada = 10
    min_decada = horas*dias*anos*decada
    escreva("Em uma decada possui ", min_decada, " minutos")
  }
}
